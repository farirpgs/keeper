{
  "name": "afraid-aperture",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "postinstall": "bun add -g netlify-cli",
    "dev": "bun x --bun astro dev",
    "start": "bun x --bun astro dev",
    "build": "bun check && bun x --bun astro build",
    "check": "bun x --bun astro check --minimumSeverity warning",
    "preview": "bun x --bun astro preview",
    "astro": "bun x --bun astro",
    "alpha": "netlify build && netlify deploy",
    "prod": "netlify build && netlify deploy --prod",
    "content": "bun ./scripts/content.ts",
    "knip": "knip"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/mdx": "4.0.3",
    "@astrojs/netlify": "6.0.1",
    "@astrojs/partytown": "^2.1.3",
    "@astrojs/starlight": "^0.30.4",
    "@astrojs/react": "4.1.2",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/tailwind": "5.1.4",
    "@mdx-js/mdx": "^3.1.0",
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/themes": "^3.1.6",
    "@tailwindcss/typography": "^0.5.16",
    "@types/bun": "^1.1.15",
    "@types/canvas-confetti": "^1.9.0",
    "@types/lodash": "^4.17.14",
    "@types/prompts": "^2.4.9",
    "@types/react": "^19.0.3",
    "@types/react-dom": "^19.0.2",
    "@vercel/og": "^0.6.4",
    "astro": "5.1.3",
    "canvas-confetti": "^1.9.3",
    "clsx": "^2.1.1",
    "github-slugger": "^2.0.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.469.0",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "prompts": "^2.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "knip": "^5.41.1"
  }
}
